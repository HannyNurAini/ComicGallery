@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <div>
        <h1>@ViewBag.Title</h1>
        <h2>Issue #@ViewBag.issueNumber</h2>


        <h5>Description:</h5>
        <div>@Html.Raw(ViewBag.description)</div>

        <h5>Artist:</h5>
        <div>
            <ul>
                @foreach (var a in ViewBag.artists)
                {
                    @a<br>
                }
            </ul>
        </div>
    </div>
</body>
</html>
